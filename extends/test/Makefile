EXES_BASIC_MULTIPLE_URL_ENGINE_BATCH = MultipleURLEngineBatchTest
EXES_HTTP_BENCHMARK = HttpBenchmark

CFLAGS +=
CXXFLAGS +=
CPPFLAGS += -g -I.. -I../../base -I../../proto -I../../proto/msgpack -I../../proto/rapidjson -I../../core
LIBS += -L.. -L../../base -L../../proto -L../../core -lextends -lssl -lcrypto

OBJS_CLIENT = proxyTest.o
OBJS_CLIENT2 = FPZKProxyTest.o
OBJS_SERVER2 = serverWithFPZK.o
