	$(RM) *.o $(EXES_CLIENT) $(EXES_FPZK_CLIENT) $(EXES_FPZK_SERVER) $(EXES_FPZK_TEST_SERVER) $(EXES_BASIC_MULTIPLE_URL_ENGINE) $(EXES_BASIC_MULTIPLE_URL_ENGINE_MORE_CONN_INFO) $(EXES_BASIC_MULTIPLE_URL_ENGINE_BATCH) $(EXES_HTTP_BENCHMARK)
	-$(RM) -rf *.dSYM
	
